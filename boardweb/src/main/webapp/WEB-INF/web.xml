<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>boardweb</display-name>
  
  <!-- welcome.do의 요청이 들어오면 co.yedam.WelcomeServlet을 실행시킴. -->
  <servlet>
  	<servlet-name>welcome</servlet-name>
  	<servlet-class>co.yedam.WelcomeServlet</servlet-class>
  </servlet>

  <servlet-mapping> 
  <servlet-name>welcome</servlet-name>
  <url-pattern>/welcome.do</url-pattern>
  </servlet-mapping>
  
 
 
  <!--  .do로 끝나면 전부 프론트컨트롤을 실행.-->
  <servlet>
  <servlet-name>front</servlet-name>
  <servlet-class>co.yedam.common.FrontController</servlet-class>
  </servlet>
   <servlet-mapping> 
  <servlet-name>front</servlet-name>
  <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  
  
  
  <welcome-file-list>
  	
  	<welcome-file>index.html</welcome-file>
  	
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>









